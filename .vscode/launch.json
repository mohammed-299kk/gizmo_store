{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flutter Web (Fast)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "-d",
                "chrome",
                "--web-renderer",
                "html",
                "--web-port",
                "8080",
                "--dart-define=FLUTTER_WEB_USE_SKIA=false"
            ]
        },
        {
            "name": "Flutter Web (Debug)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "-d",
                "chrome",
                "--debug"
            ]
        }
    ]
}
